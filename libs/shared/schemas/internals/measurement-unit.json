{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "MeasurementUnit",
  "type": "string",
  "description": "https://en.wikipedia.org/wiki/International_System_of_Units#Units_and_prefixes",
  "oneOf": [
    {
      "$ref": "#/$defs/BaseUnit"
    },
    {
      "$ref": "#/$defs/DerivedUnit"
    },
    {
