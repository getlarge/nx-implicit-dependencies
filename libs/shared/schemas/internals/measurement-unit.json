    }
  ],
  "$defs": {
    "BaseUnit": {
      "title": "BaseUnit",
      "type": "string",
      "description": "SI base unit",
      "enum": ["m", "kg", "s", "A", "K", "mol", "cd"]
    },
    "DerivedUnit": {
